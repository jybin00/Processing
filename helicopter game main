Mover m;

void setup() {
  size(640 , 320);
  m = new Mover();
}

void draw() {
  background(255);
  
  PVector f = new PVector(0,0.1);
  m.applyForce(f);
  m.wind();
  m.edges();
  m.display();
  m.update();
  println("acceleration : " +m.acceleration.x, m.acceleration.y);
  println(m.velocity.x,m.velocity.y);
  println(m.location.x,m.location.x);
}
